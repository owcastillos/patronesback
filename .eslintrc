{
  "extends": ["eslint:recommended", "google","plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": [2, 120, 8]
  },
  "env": {
    "mocha": true,
    "amd": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
