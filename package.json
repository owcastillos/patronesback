    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "repack-zip": "^0.2.8",
    "sinon": "^14.0.1"
